{
  "name": "point-sprint",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:check": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "lint:check": "eslint",
    "lint:fix": "eslint --fix",
    "type:performance": "tsc --generateTrace trace-output --incremental false",
    "type:check": "tsc --noEmit",
    "spell:check": "cspell . --gitignore --config cspell.config.yaml",
    "test": "vitest --coverage",
    "test:check": "vitest run",
    "check:all": "bun run format:check && bun run lint:check && bun run type:check && bun run spell:check && bun run test:check",
    "fix:all": "bun run format:fix && bun run lint:fix",
    "theme": "yamada-cli tokens ./src/theme/index.ts",
    "theme:watch": "yamada-cli tokens ./src/theme/index.ts --watch",
    "prepare": "bun theme",
    "depcheck": "bunx knip"
  },
  "dependencies": {
    "@conform-to/react": "^1.2.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@hello-pangea/dnd": "^17.0.0",
    "@mobily/ts-belt": "^3.13.1",
    "@valibot/i18n": "^1.0.0-beta.2",
    "@yamada-ui/fontawesome": "^1.0.52",
    "@yamada-ui/lucide": "^1.10.2",
    "@yamada-ui/react": "^1.7.3",
    "@yamada-ui/table": "^1.3.10",
    "conform-to-valibot": "^1.13.0",
    "jotai": "^2.11.0",
    "next": "15.1.4",
    "nodemailer": "^6.9.16",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "^0.0.1",
    "valibot": "^1.0.0-beta.11",
    "valibot-env": "^0.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/eslintrc": "^3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@yamada-ui/cli": "^1.2.3",
    "babel-plugin-react-compiler": "^19.0.0-beta-e552027-20250112",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "typescript": "^5.6.0",
    "vitest": "^3.0.2"
  }
}
